	<!-- Lavender Earring -->

	<append xpath="/buffs">
		<buff name="buffaccessoryLavenderEarring" name_key="buffaccessoryLavenderEarringName" description_key="buffaccessoryLavenderEarringDesc" showonhud="false" icon="noarmour" remove_on_death="false">
			<stack_type value="ignore"/>
			<update_rate value="0.5"/>
			<effect_group>
				<passive_effect name="HypothermalResist" operation="base_add" value="3"/>
				<passive_effect name="HyperthermalResist" operation="base_add" value="3"/>
				<passive_effect name="BuffBlink" operation="base_set" value="2" duration="0,10" tags="buffaccessoryLavenderEarring"/>
			</effect_group>

			<effect_group>
				<passive_effect name="HealthLoss" operation="perc_add" value="0.25"/>
			</effect_group>

			<effect_group>
				<triggered_effect trigger="onSelfAttackedOther" action="AddBuff" buff="buffaccessoryLavenderEarring_Leech"/>
				<triggered_effect trigger="onSelfAttackedOther" action="AddBuff" buff="buffaccessoryLavenderEarring_Sound"/>
			</effect_group>
		</buff>

		<buff name="buffaccessoryLavenderEarring_Leech" hidden="true">
