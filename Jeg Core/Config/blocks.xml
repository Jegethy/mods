<config>

	<!-- New player beds -->
	<append xpath="/blocks">
		<block name="bed02Player">
			<property name="DescriptionKey" value="bedGroupDesc"/>
			<property name="Class" value="SleepingBag"/>
			<property name="Material" value="Mfurniture"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Model" value="Entities/Furniture/kingSizedBedPrefab"/>
			<property name="DisplayType" value="blockMulti"/>
			<property name="MultiBlockDim" value="2,1,2"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="FallDamage" value=".6"/>
			<property name="CanPickup" value="true" param1="bedrollBlockVariantHelper"/>
			<property class="RepairItems">
				<property name="resourceCloth" value="40"/>
				<property name="resourceSpring" value="20"/>
				<property name="resourceWood" value="20"/>
				<property name="resourceMetalPipe" value="10"/>
			</property>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceCloth" count="16" tag="allHarvest"/>
			<drop event="Harvest" name="resourceWood" count="8" tag="allHarvest"/>
			<drop event="Harvest" name="resourceSpring" count="6" tag="salvageHarvest"/>
			<drop event="Harvest" name="resourceMetalPipe" count="2,3" tag="salvageHarvest"/>
			<drop event="Destroy" name="resourceScrapIron" count="10,15"/>
			<property name="Group" value="Decor/Miscellaneous"/>
			<property name="EconomicValue" value="2600"/>
			<property name="FilterTags" value="MC_building,SC_decor"/>
			<property name="CustomIcon" value="bed02"/>
		</block>
	</append>
	
	<append xpath="/blocks">
		<block name="bunkBedMadePlayer">
			<property name="DescriptionKey" value="bedGroupDesc"/>
			<property name="Class" value="SleepingBag"/>
			<property name="Material" value="Mmetal_weak"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Model" value="Entities/Furniture/bedMadePrefab"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="Texture" value="282"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="DisplayType" value="blockMulti"/>
			<property name="MultiBlockDim" value="1,1,2"/>
			<property name="FallDamage" value=".6"/>
			<property name="CanPickup" value="true" param1="bedrollBlockVariantHelper"/>
			<property name="OnlySimpleRotations" value="true"/>
			<property name="FuelValue" value="6"/>
			<property name="DowngradeBlock" value="bunkBedFrame"/>
			<property class="RepairItems">
				<property name="resourceCloth" value="10"/>
			</property>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceCloth" count="5" tag="salvageHarvest"/>
			<drop event="Destroy" name="resourceCloth" count="5"/>
			<property name="Group" value="Decor/Miscellaneous"/>
			<property name="EconomicValue" value="1330"/>
			<property name="FilterTags" value="MC_building,SC_decor"/>
			<property name="CustomIcon" value="bunkBedMade"/>
		</block>
	</append>
	
	<insertAfter xpath="/blocks/block[@name='bed02']/property[@name='FallDamage']">
		<property name="CanPickup" value="false"/>
	</insertAfter>

	<!-- Adds Stainless Steel back in -->
	<append xpath="/blocks">
		<block name="stainlessSteelShapes" shapes="All">
			<property name="Group" value="Building,advBuilding"/>
			<property name="DescriptionKey" value="stainlessSteelBlockGroupDesc"/>
			<property name="Material" value="MstainlessSteel_shapes"/>
			<property name="DisplayType" value="blockHardened"/>
			<property name="PassThroughDamage" value="true"/>
			<property name="Texture" value="191"/>
			<property name="UiBackgroundTexture" value="191"/>
			<property class="RepairItems">
				<property name="resourceForgedSteel" value="10"/>
			</property>
			<property name="CustomUpgradeSound" value="place_block_metal"/>
			<drop event="Harvest" name="resourceScrapIron" count="15" tag="allHarvest"/>
			<drop event="Destroy" count="0"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
			<property name="EconomicValue" value="240"/>
			<property name="FilterTags" value="MC_Shapes"/>
			<property name="SortOrder1" value="0011"/>
		</block>
	</append>

	<append xpath="/blocks/block[@name='steelShapes']">
		<property class="UpgradeBlock">
			<property name="ToBlock" value="stainlessSteelShapes"/>
			<property name="Item" value="resourceForgedSteel"/>
			<property name="ItemCount" value="10"/>
			<property name="UpgradeHitCount" value="4"/>
		</property>
	</append>
	
	<!-- Wood, iron and steel bars -->
	<append xpath="/blocks">
		<!-- Wood variants -->
		<block name="jegBlockWoodBarsEdge">
			<property name="Extends" value="ironBarsCentered" param1="RepairItems"/>
			<property name="StabilitySupport" value="true"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Collide" value="melee,movement,rocket"/>
			<property name="Material" value="Mwood"/>
			<property name="MaxDamage" value="500"/>
			<property name="EconomicValue" value="10"/>
			<property name="DescriptionKey" value="jegBlockWoodBarsDesc"/>
			<property class="RepairItems">
				<property name="resourceWood" value="20"/>
			</property>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockIronBarsEdge"/>
				<property name="Item" value="resourceForgedIron"/>
				<property name="ItemCount" value="15"/>
				<property name="UpgradeHitCount" value="8"/>
			</property>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?WoodBarsEdgePrefab"/>
			<property name="CustomIcon" value="jegWoodEdge"/>
		</block>
		<block name="jegBlockWoodBarsCorner">
			<property name="Extends" value="jegBlockWoodBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?WoodBarsCornerPrefab"/>
			<property name="CustomIcon" value="jegWoodCorner"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockIronBarsCorner"/>
			</property>
		</block>
		<block name="jegBlockWoodBarsCenterEdge">
			<property name="Extends" value="jegBlockWoodBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?WoodBarsCenterEdgePrefab"/>
			<property name="CustomIcon" value="jegWoodCenterEdge"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockIronBarsCenterEdge"/>
			</property>
		</block>
		<block name="jegBlockWoodBarsCenterCorner">
			<property name="Extends" value="jegBlockWoodBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?WoodBarsCenterCornerPrefab"/>
			<property name="CustomIcon" value="jegWoodCenterCorner"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockIronBarsCenterCorner"/>
			</property>
		</block>
		<block name="jegBlockWoodBarsVariantHelper">
			<property name="Extends" value="jegBlockWoodBarsEdge"/>
			<property name="CustomIcon" value="jegWoodEdge"/>
			<property name="ItemTypeIcon" value="all_blocks"/>
			<property name="SelectAlternates" value="true"/>
			<property name="PlaceAltBlockValue" value="jegBlockWoodBarsEdge,jegBlockWoodBarsCorner,jegBlockWoodBarsCenterEdge,jegBlockWoodBarsCenterCorner"/>
			<property name="DescriptionKey" value="blockVariantHelperGroupDesc"/>
		</block>

		<!-- Iron variants -->
		<block name="jegBlockIronBarsEdge">
			<property name="Extends" value="ironBarsCentered" param1="RepairItems"/>
			<property name="StabilitySupport" value="true"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Collide" value="melee,movement,rocket"/>
			<property name="Material" value="Miron"/>
			<property name="MaxDamage" value="4000"/>
			<property name="EconomicValue" value="20"/>
			<property name="DescriptionKey" value="jegBlockIronBarsDesc"/>
			<property class="RepairItems">
				<property name="resourceForgedIron" value="12"/>
			</property>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockSteelBarsEdge"/>
				<property name="Item" value="resourceForgedSteel"/>
				<property name="ItemCount" value="15"/>
				<property name="UpgradeHitCount" value="12"/>
			</property>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?IronBarsEdgePrefab"/>
			<drop event="Harvest" name="resourceForgedIron" count="0,3" prob="0.2" tag="allHarvest"/>
			<drop event="Harvest" name="resourceScrapIron" count="16,32" prob="0.8" tag="allHarvest"/>
			<drop event="Destroy" name="resourceScrapIron" count="16,32"/>
			<property name="CustomIcon" value="jegIronEdge"/>
		</block>
		<block name="jegBlockIronBarsCorner">
			<property name="Extends" value="jegBlockIronBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?IronBarsCornerPrefab"/>
			<property name="CustomIcon" value="jegIronCorner"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockSteelBarsCorner"/>
			</property>
		</block>
		<block name="jegBlockIronBarsCenterEdge">
			<property name="Extends" value="jegBlockIronBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?IronBarsCenterEdgePrefab"/>
			<property name="CustomIcon" value="jegIronCenterEdge"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockSteelBarsCenterEdge"/>
			</property>
		</block>
		<block name="jegBlockIronBarsCenterCorner">
			<property name="Extends" value="jegBlockIronBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?IronBarsCenterCornerPrefab"/>
			<property name="CustomIcon" value="jegIronCenterCorner"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="jegBlockSteelBarsCenterCorner"/>
			</property>
		</block>
		<block name="jegBlockIronBarsVariantHelper">
			<property name="Extends" value="jegBlockIronBarsEdge"/>
			<property name="CustomIcon" value="jegIronEdge"/>
			<property name="ItemTypeIcon" value="all_blocks"/>
			<property name="SelectAlternates" value="true"/>
			<property name="PlaceAltBlockValue" value="jegBlockIronBarsEdge,jegBlockIronBarsCorner,jegBlockIronBarsCenterEdge,jegBlockIronBarsCenterCorner"/>
			<property name="DescriptionKey" value="blockVariantHelperGroupDesc"/>
		</block>

		<!-- Steel variants -->
		<block name="jegBlockSteelBarsEdge">
			<property name="Extends" value="ironBarsCentered" param1="RepairItems"/>
			<property name="StabilitySupport" value="true"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Collide" value="melee,movement,rocket"/>
			<property name="Material" value="Mmetal_hard2"/>
			<property name="MaxDamage" value="12000"/>
			<property name="EconomicValue" value="50"/>
			<property name="DescriptionKey" value="jegBlockSteelBarsDesc"/>
			<property class="RepairItems">
				<property name="resourceForgedSteel" value="12"/>
			</property>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?SteelBarsEdgePrefab"/>
			<drop event="Harvest" name="resourceForgedSteel" count="0,3" prob="0.2" tag="allHarvest"/>
			<drop event="Harvest" name="resourceScrapIron" count="24,48" prob="0.8" tag="allHarvest"/>
			<drop event="Destroy" name="resourceScrapIron" count="24,48"/>
			<property name="CustomIcon" value="jegSteelEdge"/>
		</block>
		<block name="jegBlockSteelBarsCorner">
			<property name="Extends" value="jegBlockSteelBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?SteelBarsCornerPrefab"/>
			<property name="CustomIcon" value="jegSteelCorner"/>
		</block>
		<block name="jegBlockSteelBarsCenterEdge">
			<property name="Extends" value="jegBlockSteelBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?SteelBarsCenterEdgePrefab"/>
			<property name="CustomIcon" value="jegSteelCenterEdge"/>
		</block>
		<block name="jegBlockSteelBarsCenterCorner">
			<property name="Extends" value="jegBlockSteelBarsEdge"/>
			<property name="Model" value="#@modfolder:Resources/jegSteelBars.unity3d?SteelBarsCenterCornerPrefab"/>
			<property name="CustomIcon" value="jegSteelCenterCorner"/>
		</block>
		<block name="jegBlockSteelBarsVariantHelper">
			<property name="Extends" value="jegBlockSteelBarsEdge"/>
			<property name="CustomIcon" value="jegSteelEdge"/>
			<property name="ItemTypeIcon" value="all_blocks"/>
			<property name="SelectAlternates" value="true"/>
			<property name="PlaceAltBlockValue" value="jegBlockSteelBarsEdge,jegBlockSteelBarsCorner,jegBlockSteelBarsCenterEdge,jegBlockSteelBarsCenterCorner"/>
			<property name="DescriptionKey" value="blockVariantHelperGroupDesc"/>
		</block>
	</append>

	<!-- Wheat from tall grass -->
	<insertAfter xpath="/blocks/block[@name='treeGrassMaster']/property[@name='HarvestOverdamage']">
		<drop event="Harvest" name="foodCropWheat" prob=".33" count="1,4" tag="allHarvest,wildCropsHarvest"/>
	</insertAfter>
	<!-- Wheat from cactus -->
	<insertAfter xpath="/blocks/block[@name='treeCactus01']/property[@name='CanPlayersSpawnOn']">
		<drop event="Harvest" name="foodCropWheat" prob=".33" count="2,10" tag="allHarvest,wildCropsHarvest"/>
	</insertAfter>

	<!-- Triples the fuel capacity of generators -->
	<set xpath="/blocks/block[@name='generatorbank']/property[@name='MaxFuel']/@value">8000</set>

	<!-- Changes bulletproof glass repair item -->
	<set xpath="/blocks/block[@name='glassBulletproofMaster']/property[@class='RepairItems']/property[@name='glassBulletproofBlockVariantHelper']/@name">resourceForgedSteel</set>
	<set xpath="/blocks/block[@name='windowStoreCornerBulletproof']/property[@class='RepairItems']/property[@name='glassBulletproofBlockVariantHelper']/@name">resourceForgedSteel</set>
	<set xpath="/blocks/block[@name='glassBulletproofMaster']/property[@class='RepairItems']/property[@name='resourceForgedSteel']/@value">10</set>
	<set xpath="/blocks/block[@name='windowStoreCornerBulletproof']/property[@class='RepairItems']/property[@name='resourceForgedSteel']/@value">10</set>

	<!-- Shotgun & SMG turret buffs 1.3 -->
	<set xpath="/blocks/block[@name='shotgunTurret']/property[@name='BurstRoundCount']/@value">8</set>
	<set xpath="/blocks/block[@name='shotgunTurret']/property[@name='MaxDistance']/@value">30</set>
	<set xpath="/blocks/block[@name='shotgunTurret']/property[@name='EntityDamage']/@value">20</set>
	<set xpath="/blocks/block[@name='shotgunTurret']/property[@name='BlockDamage']/@value">0</set>
	<set xpath="/blocks/block[@name='shotgunTurret']/property[@name='RaySpread']/@value">0</set>

	<set xpath="/blocks/block[@name='autoTurret']/property[@name='BurstRoundCount']/@value">30</set>
	<set xpath="/blocks/block[@name='autoTurret']/property[@name='MaxDistance']/@value">45</set>
	<set xpath="/blocks/block[@name='autoTurret']/property[@name='EntityDamage']/@value">48</set>
	<set xpath="/blocks/block[@name='autoTurret']/property[@name='BlockDamage']/@value">0</set>
	<set xpath="/blocks/block[@name='autoTurret']/property[@name='RaySpread']/@value">0</set>

	<remove xpath="/blocks/block[@name='autoTurret']/property[@name='WakeUpSound']"/>
	<remove xpath="/blocks/block[@name='autoTurret']/property[@name='FireSound']"/>
	<remove xpath="/blocks/block[@name='autoTurret']/property[@name='TargetingSound']"/>
	<remove xpath="/blocks/block[@name='autoTurret']/property[@name='OverheatSound']"/>
	<remove xpath="/blocks/block[@name='shotgunTurret']/property[@name='FireSound']"/>

	<set xpath="/blocks/block[@name='autoTurret']/property[@name='Material']/@value">Mmetal_harder</set>

	<!-- Iron drops ore again -->
	<set xpath="/blocks/block[@name='terrOreIron']/drop[@event='Harvest' and @name='resourceScrapIron']/@count">25</set>
	<set xpath="/blocks/block[@name='terrOreIron']/drop[@event='Harvest' and @name='resourceScrapIron']/@name">resourceIronFragment</set>
	<set xpath="/blocks/block[@name='terrOreIron']/drop[@event='Fall' and @name='resourceScrapIron']/@name">resourceIronFragment</set>

	<set xpath="/blocks/block[@name='oreIronBoulder']/drop[@event='Harvest' and @name='resourceScrapIron']/@count">25</set>
	<set xpath="/blocks/block[@name='oreIronBoulder']/drop[@event='Harvest' and @name='resourceScrapIron']/@name">resourceIronFragment</set>

	<append xpath="/blocks">
		<block name="reinforcedBladeTrap">
			<property name="CreativeMode" value="Player"/>
			<property name="CustomIcon" value="bladeTrap"/>
			<property name="CustomIconTint" value="B0E7FF"/>
			<property name="name_key" value="reinforcedBladeTrap"/>
			<property name="description_key" value="reinforcedBladeTrapDesc"/>
			<property name="Class" value="BladeTrap"/>
			<property name="UnlockedBy" value="perkAdvancedEngineering,bladeTrapSchematic"/>
			<property name="Material" value="Mmetal_hardest"/>
			<property name="Damage" value="50"/>
			<property name="DamageReceived" value="4"/>
			<property name="BreakingPercentage" value="0.5"/>
			<property name="BrokenPercentage" value="0.25"/>
			<property name="ImpactSound" value="bladetrap_impact"/>
			<property name="StartSound" value="bladetrap_startup"/>
			<property name="StopSound" value="bladetrap_stop"/>
			<property name="RunningSound" value="bladetrap_fire_lp"/>
			<property name="RunningSoundBreaking" value="bladetrap_dm1_lp"/>
			<property name="RunningSoundBroken" value="bladetrap_dm2_lp"/>
			<property name="MaxDamage" value="75000"/>
			<property name="StabilitySupport" value="false"/><!-- build restriction -->
			<property name="Shape" value="ModelEntity"/>
			<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
			<property name="Model" value="Entities/Electrical/trap_spinning_bladePrefab"/><property name="HandleFace" value="Bottom"/>
			<property name="MultiBlockDim" value="3,1,3"/><property name="DisplayType" value="blockElectricalHardenedMulti"/>
			<property name="ImposterExchange" value="imposterPlate" param1="111"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
			<property name="RequiredPower" value="5"/>
			<property name="Group" value="Tools/Traps,Science"/>
			<property name="ActivationDistance" value="6"/>
			<property class="RepairItems">
				<property name="resourceForgedSteel" value="16"/>
			</property>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceScrapIron" count="20,100" tag="allHarvest"/>
			<drop event="Destroy" count="0"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
			<property name="Stacknumber" value="500"/>
			<property name="EconomicValue" value="444"/>
			<property name="FilterTags" value="fdecor,felectrical,ftraps"/>
			<property name="SortOrder1" value="70a0"/>
		</block>
	</append>

	<append xpath="/blocks">
		<block name="Wireless Junction Box">
			<property name="Class" value="ElectricWire"/>
			<property name="Material" value="Mmetal_hardest"/>
			<property name="StabilitySupport" value="false"/>
			<property name="Texture" value="8"/>
			<property name="Shape" value="Ext3dModel"/><property name="Texture" value="293"/>
			<property name="Mesh" value="models"/>
			<property name="Model" value="Industrial/fusebox" param1="main_mesh"/><property name="HandleFace" value="South"/>
			<property name="MaxDamage" value="25000"/>
			<property name="DamageReceived" value="0"/>
			<property name="CustomIcon" value="fusebox"/>
			<property name="CustomIconTint" value="B0E7FF"/>
			<property name="ImposterDontBlock" value="true"/>
			<property name="PoweredType" value="Constant"/>
			<property name="RequiredPower" value="0"/>
			<property name="Group" value="Science"/>
			<property class="RepairItems"><property name="resourceForgedIron" value="10"/></property>
			<drop event="Harvest" name="electricParts" count="0,1" tool_category="Disassemble"/>
			<drop event="Harvest" name="scrapIron" count="5,40"/>
			<drop event="Destroy" count="0"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
			<property name="EconomicValue" value="62"/>
			<property name="FilterTags" value="fdecor,fother,felectrical"/>
			<property name="SortOrder1" value="70e0"/>
		</block>
	</append>
	
	<set xpath="/blocks/block[@name='bedrollBlockVariantHelper']/property[@name='PlaceAltBlockValue']/@value">bedroll,bedrollRed,bedrollOrange,bedrollYellow,bedrollGreen,bedrollBlue,bedrollPurple,bedrollPink,bedrollGrey,bedrollWhite,bedrollArmyGreen,bed02Player,bunkBedMadePlayer</set>

	<set xpath="/blocks/block[@name='rockResource']/drop[@event='Harvest']/@count">12</set>
	<set xpath="/blocks/block[@name='terrDesertGround']/drop[@name='resourceCrushedSand']/@count">12</set>
	<set xpath="/blocks/block[@name='terrDesertGround']/drop[@name='resourceClayLump']/@count">6</set>

	<!-- Electric fence posts have more health -->
	<set xpath="/blocks/block[@name='electricfencepost']/property[@name='DamageReceived']/@value">0</set>
	<set xpath="/blocks/block[@name='electricfencepost']/property[@name='MaxDamage']/@value">2500</set>
	
	<!-- Adds silver and gold coins to loot -->
	<insertAfter xpath="/blocks/block[@name='terrDirt']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrForestGround']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrBurntForestGround']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrDesertGround']/property[@name='Map.Color']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrSand']/property[@name='Map.Color']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrSnow']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrGravel']/property[@name='Map.Color']">
		<drop event="Harvest" name="resourcedOldGoldCoin" count="1" prob="0.015" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrDirt']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldSilverCoin" count="1" prob="0.020" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrForestGround']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldSilverCoin" count="1" prob="0.020" tag="allHarvest"/>
	</insertAfter>

	<insertAfter xpath="/blocks/block[@name='terrBurntForestGround']/property[@class='RepairItems']">
		<drop event="Harvest" name="resourcedOldSilverCoin" count="1" prob="0.020" tag="allHarvest"/>
	</insertAfter>

