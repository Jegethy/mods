				<passive_effect name="BuffBlink" operation="base_set" value="2" duration="0,10" tags="buffaccessoryCombinedRings"/>
			</effect_group>

			<effect_group>
				<passive_effect name="HarvestCount" operation="perc_add" value="5" tags="oreWoodHarvest"/>
				<passive_effect name="CarryCapacity" operation="base_set" value="3"/>
				<passive_effect name="StaminaChangeOT" operation="perc_subtract" value="0.3"/>
			</effect_group>
		</buff>
	</append>

	<!-- Crimson Earring - 300% Headshot damage, 0 body damage-->

	<append xpath="/buffs">
		<buff name="buffaccessoryCrimsonEarring" name_key="buffaccessoryCrimsonEarringName" description_key="buffaccessoryCrimsonEarringDesc" showonhud="false" icon="headshotsonlybuff" remove_on_death="false">
